# Development Environment Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# MFEC LLM Configuration - Development
MFEC_LLM_BASE_URL=https://gpt.mfec.co.th/litellm
MFEC_LLM_CHAT_MODEL=gpt-4o
MFEC_LLM_EMBEDDING_MODEL=text-embedding-3-large

# Development Security (use weak keys for dev only)
ENCRYPTION_KEY=dev_encryption_key_32_characters_long

# Development Features
ALLOW_USER_API_KEYS=true
DEBUG_MODE=true
LOG_LEVEL=debug

# Development Performance
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=false