# Production Environment Configuration
NODE_ENV=production
NEXT_PUBLIC_APP_URL=https://thai-doc-generator.mfec.co.th

# MFEC LLM Configuration - Production
MFEC_LLM_BASE_URL=https://gpt.mfec.co.th/litellm
MFEC_LLM_CHAT_MODEL=gpt-4o
MFEC_LLM_EMBEDDING_MODEL=text-embedding-3-large

# Production Security (MUST be replaced with secure keys)
ENCRYPTION_KEY=REPLACE_WITH_SECURE_PRODUCTION_KEY

# Production Features
ALLOW_USER_API_KEYS=true
DEBUG_MODE=false
LOG_LEVEL=error

# Production Performance
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true

# Production Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=20
RATE_LIMIT_REQUESTS_PER_HOUR=200

# Production Security Headers
SECURITY_HEADERS_ENABLED=true
CORS_ORIGIN=https://thai-doc-generator.mfec.co.th