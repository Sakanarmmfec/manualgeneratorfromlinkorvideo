# Staging Environment Configuration
NODE_ENV=staging
NEXT_PUBLIC_APP_URL=https://thai-doc-generator-staging.example.com

# MFEC LLM Configuration - Staging
MFEC_LLM_BASE_URL=https://gpt.mfec.co.th/litellm
MFEC_LLM_CHAT_MODEL=gpt-4o
MFEC_LLM_EMBEDDING_MODEL=text-embedding-3-large

# Staging Security (use secure keys in actual deployment)
ENCRYPTION_KEY=staging_encryption_key_replace_with_secure_key

# Staging Features
ALLOW_USER_API_KEYS=true
DEBUG_MODE=false
LOG_LEVEL=info

# Staging Performance
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true

# Staging Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=30
RATE_LIMIT_REQUESTS_PER_HOUR=500